{
  "expo": {
    "name": "Laisvalaikio preki≈≥ dalinimosi platforma",
    "slug": "Client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/favicon.png",
    "scheme": "com.laisvalaikio.app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "deepLinking": {
      "url": "com.laisvalaikio.app",
      "prefixes": [
        "com.laisvalaikio.app://",
        "https://laisvalaikio.app"
      ],
      "config": {
        "screens": {
          "google-callback": "google-callback/:code",
          "facebook-callback": "facebook-callback/:code",
          "login": "(auth)/login",
          "register": "(auth)/register"
        }
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.redasn.Client"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "http://10.51.21.135:8081"
      },
      "API_BASE_URL": "http://10.51.21.135:5050"
    }
  }
}